{% extends 'base.html.twig' %}

{% block title %}Data{% endblock %}

{% block body %}
    <h1>Data</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ data.id }}</td>
            </tr>
            <tr>
                <th>Symbol</th>
                <td>{{ data.symbol }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ data.name }}</td>
            </tr>
            <tr>
                <th>Symbol_native</th>
                <td>{{ data.symbolNative }}</td>
            </tr>
            <tr>
                <th>Decimal_digits</th>
                <td>{{ data.decimalDigits }}</td>
            </tr>
            <tr>
                <th>Rounding</th>
                <td>{{ data.rounding }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ data.code }}</td>
            </tr>
            <tr>
                <th>Name_plural</th>
                <td>{{ data.namePlural }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ data.type }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_data_index') }}">back to list</a>

    <a href="{{ path('app_data_edit', {'id': data.id}) }}">edit</a>

    {{ include('data/_delete_form.html.twig') }}
{% endblock %}
